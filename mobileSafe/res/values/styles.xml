<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="set_safePass_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/main_tittle_background</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="input_safePass_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/main_tittle_background</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="setguid_title_txt">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#00f700</item>
    </style>

    <style name="setguid_divide_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2dip</item>
        <item name="android:src">@drawable/divid_line</item>
    </style>

    <style name="setguid_starImg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@android:drawable/btn_star_big_on</item>
    </style>
    
    <style name="setguid_txt_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
         <item name="android:textColor">#00f700</item>
         <item name="android:gravity">bottom</item>
          <item name="android:textSize">22sp</item>
    </style>
    
     <style name="setguid_current_progress_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
          <item name="android:src">@drawable/jog_tab_target_green</item>
    </style>
    
     <style name="setguid_progress_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
          <item name="android:src">@drawable/jog_tab_target_gray</item>
    </style>
    
      <style name="lv_item_contact_tv">
        <item name="android:layout_height">wrap_content</item>
          <item name="android:textColor">#fff7f3f7</item>
          <item name="android:textSize">15sp</item>
    </style>
    
      <style name="ll_title">
        <item name="android:layout_height">wrap_content</item>
          <item name="android:textColor">#fff7f3f7</item>
          <item name="android:textSize">15sp</item>
    </style>
    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>